<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.smishlyaevakatya_navigation.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_navigation_home_to_navigation_dashboard2"
            app:destination="@id/navigation_dashboard"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_navigation_home_to_navigation_notifications2"
            app:destination="@id/navigation_notifications"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_right"
            app:popExitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_navigation_home_to_navigation_furniture"
            app:destination="@id/navigation"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_navigation_home_to_factsFragment"
            app:destination="@id/factsFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
    </fragment>

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.smishlyaevakatya_navigation.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard">
        <action
            android:id="@+id/action_navigation_dashboard_to_navigation_home2"
            app:destination="@id/navigation_home"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right"
            app:popEnterAnim="@anim/from_right"
            app:popExitAnim="@anim/to_left" />
        <action
            android:id="@+id/action_navigation_dashboard_to_navigation_furniture"
            app:destination="@id/navigation"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
    </fragment>

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.smishlyaevakatya_navigation.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications">
        <action
            android:id="@+id/action_navigation_notifications_to_navigation_home2"
            app:destination="@id/navigation_home" />
    </fragment>

    <navigation android:id="@+id/navigation"
        app:startDestination="@id/navigation_furniture">
        <fragment
            android:id="@+id/navigation_furniture"
            android:name="com.example.smishlyaevakatya_navigation.ui.homework25.FurnitureFragment"
            android:label="@string/title_furniture"
            tools:layout="@layout/fragment_furniture">
            <action
                android:id="@+id/action_navigation_furniture_to_navigation_home"
                app:destination="@id/navigation_home"
                app:enterAnim="@anim/from_left"
                app:exitAnim="@anim/to_right"
                app:popEnterAnim="@anim/from_right"
                app:popExitAnim="@anim/to_left" />
            <action
                android:id="@+id/action_navigation_furniture_to_navigation_item_furniture"
                app:destination="@id/navigation2"
                app:enterAnim="@anim/from_right"
                app:exitAnim="@anim/to_left"
                app:popEnterAnim="@anim/from_left"
                app:popExitAnim="@anim/to_right" />
            <argument
                android:name="itemFurniture"
                app:argType="com.example.smishlyaevakatya_navigation.ui.homework25.Furniture"
                app:nullable="true"
                android:defaultValue="@null" />
        </fragment>
        <action
            android:id="@+id/action_global_insertFurnitureFragment"
            app:destination="@id/insertFurnitureFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_global_updateFragment"
            app:destination="@id/updateFragment" />
    </navigation>
    <navigation android:id="@+id/navigation2"
        app:startDestination="@id/navigation_item_furniture">
        <fragment
            android:id="@+id/navigation_item_furniture"
            android:name="com.example.smishlyaevakatya_navigation.ui.homework25.ItemFragment"
            android:label="@string/title_furniture"
            tools:layout="@layout/fragment_item_new_furniture">
            <argument
                android:name="itemFurniture"
                app:argType="com.example.smishlyaevakatya_navigation.ui.homework25.Furniture" />
        </fragment>
        <action
            android:id="@+id/action_global_navigation"
            app:destination="@id/navigation" />
    </navigation>
    <fragment
        android:id="@+id/insertFurnitureFragment"
        android:name="com.example.smishlyaevakatya_navigation.ui.homework26.InsertFurnitureFragment"
        android:label="InsertFurnitureFragment" >
        <action
            android:id="@+id/action_insertFurnitureFragment_to_navigation"
            app:destination="@id/navigation" />
    </fragment>
    <fragment
        android:id="@+id/updateFragment"
        android:name="com.example.smishlyaevakatya_navigation.ui.homework26.update.UpdateFragment"
        android:label="UpdateFragment" >
        <action
            android:id="@+id/action_updateFragment_to_navigation"
            app:destination="@id/navigation" />
    </fragment>
    <fragment
        android:id="@+id/factsFragment"
        android:name="com.example.smishlyaevakatya_navigation.ui.homework27.FactsFragment"
        android:label="FactsFragment" >
        <action
            android:id="@+id/action_factsFragment_to_navigation_home"
            app:destination="@id/navigation_home"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right"
            app:popEnterAnim="@anim/from_right"
            app:popExitAnim="@anim/to_left" />
    </fragment>
</navigation>