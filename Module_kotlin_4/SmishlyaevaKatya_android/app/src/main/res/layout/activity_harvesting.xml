<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".homework18.Harvesting">


    <Button
        android:id="@+id/btn_back_harvest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_mo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Минская область"
        android:textColor="@color/teal_700"
        android:textSize="30sp"
        android:layout_marginBottom="550dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_mo_potatoes"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:text="Картофель  "
        android:textColor="@color/design_default_color_primary"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_mo" />

    <TextView
        android:id="@+id/tv_mo_cabbage"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:text="Капуста"
        android:textColor="@color/design_default_color_primary"
        android:textSize="25sp"
        app:layout_constraintRight_toLeftOf="@+id/tv_mo_potatoes"
        app:layout_constraintTop_toBottomOf="@+id/tv_mo" />

    <TextView
        android:id="@+id/tv_mo_beet"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:text="Свекла "
        android:textColor="@color/design_default_color_primary"
        android:textSize="25sp"
        app:layout_constraintLeft_toRightOf="@+id/tv_mo_potatoes"
        app:layout_constraintTop_toBottomOf="@+id/tv_mo" />

    <TextView
        android:id="@+id/tv_mo_count_pot"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_mo_potatoes" />

    <TextView
        android:id="@+id/tv_mo_count_cab"
